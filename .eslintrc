{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["error"]
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
